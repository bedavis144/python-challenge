[
    {
        "check_match": [
            "$.applications[0].borrower.mailingAddress",
            "$.applications[0].coborrower.mailingAddress"
        ],
        "target": "$.reports[?(@.title == 'Borrowers Report')].shared_address"
    },
    {
        "iterate": {
            "source_list": "$.applications",
            "target_list": "$.reports[?(@.title == 'Borrowers Report')].borrowers",
            "mappings": [
                {
                    "source": ".borrower.firstName",
                    "target": ".first_name"
                },
                {
                    "source": ".borrower.lastName",
                    "target": ".last_name"
                }
            ]
        }
    },
    {
        "iterate": {
            "source_list": "$.applications",
            "target_list": "$.reports[?(@.title == 'Borrowers Report')].borrowers",
            "mappings": [
                {
                    "source": ".coborrower.firstName",
                    "target": ".first_name"
                },
                {
                    "source": ".coborrower.lastName",
                    "target": ".last_name"
                }
            ]
        }
    }
]
